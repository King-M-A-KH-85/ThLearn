<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main"
    app:startDestination="@id/mainFragment">
    <fragment
        android:id="@+id/mainFragment"
        android:name="ir.thlearn.ui.main.MainFragment"
        android:label="main_fragment"
        tools:layout="@layout/main_fragment" >
        <action
            android:id="@+id/action_mainFragment_to_profileFragment2"
            app:destination="@id/profileFragment" />
        <action
            android:id="@+id/action_mainFragment_to_directFragment"
            app:destination="@id/directFragment" />
    </fragment>
    <fragment
        android:id="@+id/profileFragment"
        android:name="ir.thlearn.ui.Profile.ProfileFragment"
        android:label="ProfileFragment" />
    <fragment
        android:id="@+id/directFragment"
        android:name="ir.thlearn.ui.direct.DirectFragment"
        android:label="DirectFragment" />
</navigation>